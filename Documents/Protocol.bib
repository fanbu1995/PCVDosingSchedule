	shortjournal = {J Infect Dis},
	author = {Armah, George and Lewis, Kristen D. C. and Cortese, Margaret M. and Parashar, Umesh D. and Ansah, Akosua and Gazley, Lauren and Victor, John C. and {McNeal}, Monica M. and Binka, Fred and Steele, A. Duncan},
