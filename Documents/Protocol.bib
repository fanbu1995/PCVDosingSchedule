	number = {4},
	journaltitle = {Current Tropical Medicine Reports},
	shortjournal = {Curr Trop Med Rep},
	author = {Itiakorit, Harriet and Sathyamoorthi, Abhilash and O’Brien, Brigid E. and Nguyen, Diane},
