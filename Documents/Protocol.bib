	journaltitle = {{BMJ} open},
	shortjournal = {{BMJ} Open},
	author = {Oliveira, Carlos R. and Ortiz, Anette Michelle and Sheth, Sangini S. and Shapiro, Eugene D. and Niccolai, Linda M.},
	date = {2021-04-19},
	pmid = {33875441},
	pmcid = {PMC8057558},
	keywords = {Humans, Female, Vaccination, Case-Control Studies, epidemiology, public health, Uterine Cervical Neoplasms, Papillomavirus Vaccines, gynaecological oncology, Papillomavirus Infections},
